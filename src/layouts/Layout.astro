---
    interface Props {
        title: string;
    }
    import "../styles/layout.css"

    const { title } = Astro.props
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Astro Landing - { title }</title>
</head>
<body>
    <main class="container mx-auto md:pt-10">
        <slot />
    </main>
</body>
</html>